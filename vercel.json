{
  "version": 2,
  "functions": {
    "api/config.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/api/config.php",
      "dest": "/api/config.php"
    },
    {
      "src": "/(.*\\.(html|js|css|png|jpg|jpeg|svg|ico|json))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
