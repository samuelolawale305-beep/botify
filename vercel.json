{
  "version": 2,
  "functions": {
    "api/config.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/*.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "public/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/api/config.php",
      "dest": "/api/config.php"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}